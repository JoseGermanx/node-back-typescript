{
  "name": "back-end-sales-rsa",
  "version": "1.0.0",
  "description": "Back end for RSA sales administration app",
  "main": "app.ts",
  "type": "module",
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register src/app.ts | ./node_modules/.bin/bunyan",
    "lint:check": "eslint . --ext .ts",
    "lint:fix": "npx eslint --fix .",
    "prettier:check": "prettier --check src/**/*.{ts,json}",
    "prettier:fix": "prettier --write src/**/*.{ts,json}",
    "test": "jest --coverage=true -w=1 --forceExit --detectOpenHandles --watchAll=true",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "clean": "rimraf cache dist",
    "build:pro": "yarn run clean && yarn run build && gulp minifyAndOfuscate",
    "prod": ""
  },
  "author": "Jose G Martinez",
  "license": "ISC",
  "dependencies": {
    "bunyan": "^1.8.15",
    "compression": "^1.7.4",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.2.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.1.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/bunyan": "^1.8.8",
    "@types/compression": "^1.7.2",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "eslint": "8.40.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "1.8.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4",
    "typescript-transform-paths": "^3.4.6"
  }
}
